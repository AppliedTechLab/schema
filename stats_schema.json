{
  "id": "https://w3id.org/dats/schema/software_schema.json",
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "DATS summary statistics schema",
  "description": "A summary statistics schema.",
  "type": "object",
  "properties": {
    "@context": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "object"
        },
        {
          "type": "array"
        }
      ]
    },
    "statType": {
      "type": "string"
    },
    "statIRI": {
      "anyOf": [
        {
          "type": "string",
          "format": "uri"
        },
        {
          "type": "string",
          "maxLength": 0
        }
      ]
    },
    "statValues": {
      "type": "array",
      "items": {
        "type": "number"
      }
    }
  }
}